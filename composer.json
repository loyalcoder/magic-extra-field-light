{
  "name": "loyalcoder/magic-extra-field-light",
  "description": "A lightweight WordPress plugin that adds extra custom fields to WooCommerce products",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Loyalcoder",
      "email": "info@loyalcoder.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "wp-cli/wp-cli": "^2.0",
    "wp-cli/i18n-command": "^2.0"
  },
  "autoload": {
    "classmap": [
      "includes"
    ],
    "psr-4": {
      "MagicExtraFieldLight\\": "includes/"
    },
    "files": [
      "includes/functions.php"
    ]
  },
 "scripts": {
  "make-pot": "wp i18n make-pot . languages/magic-extra-field-light.pot --domain=magic-extra-field-light"
}
}
